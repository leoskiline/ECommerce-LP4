
@{
    ViewData["Title"] = "Index";
    Layout = null;
}
<link rel="stylesheet" href="~/template/klorofil/assets/vendor/bootstrap/css/bootstrap.min.css">
<script src="https://kit.fontawesome.com/3c736d02dd.js" crossorigin="anonymous"></script>
<center>
    <div class="col-md-4"></div>
    <div class="col-md-3">
            <h2 style="font-weight:bold;font-family:Cooper;color:#102d8e">LOGIN</h2>
            <form>
                <div class="form-group">
                    <input type="text" placeholder="Usuario" id="usuario" class="form-control" />
                </div>
                <div class="form-group">
                    <input type="password" placeholder="Senha" id="senha" class="form-control" />
                </div>
                <div id="divMsg" class="text-danger" style="margin-bottom:10px"></div>
                <input type="button" value="Enviar" onclick="indexLogin.logar()" class="btn btn-primary margin" />
                <input type="button" value="Obter" onclick="indexLogin.obter()" class="btn btn-info" />
                <input type="button" value="Consultar" onclick="indexLogin.consultar()" class="btn btn-success" />
                <input type="button" value="Alterar" onclick="indexLogin.alterar()" class="btn btn-warning" />
                <input type="button" value="Excluir" onclick="indexLogin.excluir()" class="btn btn-danger" />
            </form>
    </div>

</center>
<script src="~/lib/HTTPClient.js"></script>
<script src="~/js/views/login/index.js"></script>